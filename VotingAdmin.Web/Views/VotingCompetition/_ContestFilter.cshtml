<div class="card-block">
    <form id="ContestFilterForm" class="row">
        <div class="col-sm-6 col-md-4 col-lg-3">
            <div class="form-group">
                <lable>From</lable>
                <input type="date" onkeydown="return false" max="@DateTime.Now.ToString("yyyy-MM-dd")" value="@DateTime.Now.ToString("yyyy-MM-dd")" name="FromDate" id="d_today" class="form-control" placeholder="FromDate">
            </div>
        </div>
        <div class="col-sm-6 col-md-4 col-lg-3">
            <div class="form-group">
                <lable>To</lable>
                <input type="date" onkeydown="return false"  value="@DateTime.Now.ToString("yyyy-MM-dd")" name="ToDate" class="form-control" placeholder="ToDate">
            </div>
        </div>
       
        <div class="col-sm-6 col-md-4 col-lg-3">
            <div class="form-group">
                <label>Status</label>
                <select name="Status" class="form-control" asp-items="@ViewBag.Conteststatuslist">
                    <option value="">All</option>
                </select>
            </div>
        </div>
       
        <input type="hidden" id="Export" name="Export" />
        <div class="col-sm-6 col-md-2">
            <button type="submit" id="btnContestFilter" class="btn btn-md btn-primary form-control quick-btn">Filter</button>
        </div>
    </form>
</div>

<script type='text/javascript' src="~/js/ValidationScript.js"></script>